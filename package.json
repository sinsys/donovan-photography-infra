{
  "name": "donovan-infra",
  "version": "1.0.0",
  "description": "Infrastructure for Donovan Photography",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write .",
    "lint": "eslint .",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy",
    "test": "NODE_ENV=test jest --ci --verbose"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "lint-prepush": {
    "base": "develop",
    "tasks": {
      "*.{js,jsx,ts,tsx}": [
        "eslint --fix"
      ]
    }
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.122.0",
    "@aws-cdk/aws-cognito": "^1.122.0",
    "@aws-cdk/aws-dynamodb": "^1.122.0",
    "@aws-cdk/aws-lambda": "^1.122.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.122.0",
    "envalid": "^6.0.2"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.122.0",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/aws-lambda": "^8.10.17",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.17.14",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^4.30.0",
    "aws-cdk": "^1.122.0",
    "babel-jest": "^26.6.3",
    "esbuild": "^0.12.28",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "jest": "^26.6.3",
    "module-alias": "^2.2.2",
    "moq.ts": "^6.4.0",
    "prettier": "^2.3.2",
    "ts-loader": "^5.4.5",
    "typescript": "^4.4.3",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "author": {
    "name": "Nicholas Hazel <nico.full.stack.dev@gmail.com>"
